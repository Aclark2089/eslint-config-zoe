{
  "name": "eslint-config-zoe",
  "version": "1.37.1",
  "description": "ðŸŒ€ ESLint shareable config for Zoe.",
  "author": "https://github.com/jorgegonzalez",
  "license": "MIT",
  "homepage": "https://github.com/jorgegonzalez/eslint-config-zoe",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jorgegonzalez/eslint-config-zoe.git"
  },
  "bugs": {
    "url": "https://github.com/jorgegonzalez/eslint-config-zoe/issues"
  },
  "keywords": [
    "zoe",
    "code linter",
    "code style",
    "eslint",
    "formatter",
    "linter",
    "prettier",
    "react",
    "style",
    "typescript"
  ],
  "engines": {
    "node": ">=8"
  },
  "files": [
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "commitizen": "^4.0.3",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "3.0.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "semantic-release": "^15.13.31",
    "typescript": "^3.7.2",
    "zoe": "^1.38.3"
  },
  "scripts": {
    "commit": "git-cz",
    "format": "prettier --write \"*{.js,.md,.json}\"",
    "release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{.js,.md,.json}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
